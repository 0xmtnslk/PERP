modules = ["python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["cargo", "chromedriver", "chromium", "geckodriver", "libiconv", "libxcrypt", "openssl", "pkg-config", "rustc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Crypto Trading System"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Working Telegram Bot"

[[workflows.workflow]]
name = "Crypto Trading System"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main_coordinator.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Working Telegram Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 working_telegram_bot.py"

[workflows.workflow.metadata]
outputType = "console"

[deployment]
deploymentTarget = "gce"
run = ["python3", "main_coordinator.py"]
ignorePorts = true
